<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Move" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/white</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Move.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Move.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Move.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="roundimageview">
        
         <item name="cornerFamily">rounded</item>
         <item name="cornerSize">30dp</item>
         <item name="color">@color/white</item>


    </style>

    <style name="circleimageview">

        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">60dp</item>
        <item name="color">@color/white</item>


    </style>

</resources>